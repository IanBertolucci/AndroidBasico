<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        xmlns:tools="http://schemas.android.com/tools"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".VerActivity"
        android:orientation="vertical">

    <TextView
            android:id="@+id/textViewNomeResposta"
            android:layout_width="match_parent"
            android:layout_height="wrap_content" />

    <TextView
            android:id="@+id/textViewCursoResposta"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="TextView" />

    <TextView
            android:id="@+id/txtViewNota"
            android:layout_width="match_parent"
            android:layout_height="wrap_content" />

    <Button
            android:id="@+id/btnVoltar"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="@string/txtBtnVoltar" />

    <Button
            android:id="@+id/btnAvaliar"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="@string/btnTextAvaliar" />

</LinearLayout>
